# PCP Development Environment Configuration
# Copy to dev.env and fill in your values

# Environment identifier
PCP_ENV=development

# Database path (uses separate dev database)
VAULT_DB_PATH=/workspace/vault/vault_dev.db

# Discord webhook for dev channel (#sean-pcp-dev)
# Create a new webhook in your dev Discord channel
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_DEV_WEBHOOK_ID/YOUR_DEV_WEBHOOK_TOKEN

# Discord channel ID for dev
DISCORD_CHANNEL_ID=YOUR_DEV_CHANNEL_ID

# Logging
LOG_LEVEL=DEBUG

# Test mode flag
TEST_MODE=true
