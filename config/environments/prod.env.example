# PCP Production Environment Configuration
# Copy to prod.env and fill in your values

# Environment identifier
PCP_ENV=production

# Database path (production database)
VAULT_DB_PATH=/workspace/vault/vault.db

# Discord webhook for production channel (#sean-pcp)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_PROD_WEBHOOK_ID/YOUR_PROD_WEBHOOK_TOKEN

# Discord channel ID for production
DISCORD_CHANNEL_ID=YOUR_PROD_CHANNEL_ID

# Logging (less verbose in production)
LOG_LEVEL=INFO

# Test mode disabled in production
TEST_MODE=false
